using System;
using Microsoft.Contracts;

public class C 
{
  public static void Main() 
  {
    int r = doubler(5);
    System.Console.WriteLine("The double is {0}", r);
  }
  
  public static int doubler(int x) 
  ensures result == 2 * x;
  {
	  int XX;
	  XX = 2 * x;
	  assert XX == 2 * x;
	  return XX;
  }
}


